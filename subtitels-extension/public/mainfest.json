{
    "manifest_version": 3,
    "name": "Subtitels youtube extension",
    "version": "1.0",
    "description": "Extension to browser to check the translation of YouTube's subtitels.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "backgroundScript.js"
    }
  }   